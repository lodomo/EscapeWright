# SETTINGS INFO:
# name: name of the room for the control panel
# role: The name of the FILE you created for the role. (typically lowecase)
# class: The name of the CLASS you created for the role. (typically Capitalized)
# logging: True or False (if True, log file will be created)
# log_level: DEBUG, INFO, WARNING, ERROR, CRITICAL

name: sample_pi_name
role: role_template 
class: RoleTemplate 
location: sample_location
port: 12413
logging: True
log_level: DEBUG